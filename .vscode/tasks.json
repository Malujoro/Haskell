{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Haskell File",
      "type": "shell",
      "command": "/home/mateus/.ghcup/bin/runhaskell",
      "args": [
        "${file}"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    },
    {
      "label": "Open GHCI",
      "type": "shell",
      "command": "/home/mateus/.ghcup/bin/ghci",
      "args": [
        "${file}"
      ],
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "problemMatcher": []
    }
  ]
}
